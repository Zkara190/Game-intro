#include <iostream>
#include <vector>
#include <string>

using namespace std;

class bigNode {
public:
	bigNode() : backhand(NULL), deathprompt(NULL), forehand(NULL), response(NULL) {}
	void event(string r, string d, bigNode *b, bigNode *f)
	{
		backhand = b; forehand = f; response = r; deathprompt = d;
	}
	bigNode* getBack() { return backhand; }
	bigNode* getFore() { return forehand; }
	string getResponse() { return response; }
	string death() { return deathprompt; }
private:
	string response;
	string deathprompt;
	bigNode* backhand; // left node
	bigNode* forehand; // right node
};

string adventure(bigNode* T)
{
	if (T->getResponse() == "left") {
		return (adventure(T->getBack()));
	}
	else if (T->getResponse() == "right") {
		return (adventure(T->getFore()));
	}
	else return T->death();
}

